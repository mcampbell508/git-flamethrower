{
    "name": "mcampbell508/git-flamethrower",
    "description": "A tool to enhance Git commands",
    "keywords": ["git"],
    "homepage": "https://github.com/mcampbell508/git-flamethrower",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Campbell",
            "email": "matthewcampbell508@gmail.com",
            "homepage": "http://mcampbell508.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/console": "^4.0",
        "league/climate": "^3.2",
        "laravel-zero/framework": "5.6.*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "2.11"
    },
    "autoload": {
        "psr-4": {
            "MCampbell508\\Git\\Flamethrower\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MCampbell508\\Tests\\": ["tests"]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "bin": [
        "git-flamethrower"
    ]
}
